{
  "version": 2,
  "buildCommand": "bash build-vercel.sh",
  "outputDirectory": "apps/web/dist",
  "installCommand": "corepack enable && corepack prepare pnpm@latest --activate && pnpm install",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
