{
  "version": 2,
  "framework": null,
  "buildCommand": "pnpm --filter @mibudget/shared build && pnpm --filter mibudget-web build",
  "outputDirectory": "apps/web/dist",
  "installCommand": "npm install -g pnpm@latest && pnpm install --frozen-lockfile",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
